{
  "extends": [
    "config:base"
  ],
  "automerge": true,
  "prHourlyLimit": 2,
  "updateNotScheduled": false,
  "timezone": "America/New_York",
  "schedule": [
    "every weekend"
  ],
  "masterIssue": true,
  "packageRules": [
    {
      "packagePatterns": [
        "*"
      ],
      "excludePackagePatterns": [
        "debug",
        "logdown",
        "cypress",
        "semantic-release"
      ],
      "enabled": false
    }
  ]
}
